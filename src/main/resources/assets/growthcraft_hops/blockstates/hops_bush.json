{
  "multipart": [
    {
      "apply": {
        "model": "growthcraft_hops:hops_bush"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "growthcraft_hops:hops_bush_side",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "growthcraft_hops:hops_bush_side",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "growthcraft_hops:hops_bush_side",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "growthcraft_hops:hops_bush_side",
        "y": 270,
        "uvlock": false
      }
    }
  ]
}