{
  "multipart": [
    {
      "when": { "age": "0|1" },
      "apply": { "model": "growthcraft_hops:hops_bush_stage0" }
    },
    {
      "when": { "age": "1" },
      "apply": { "model": "growthcraft_hops:hops_bush_stage1" }
    },
    {
      "when": { "age": "2" },
      "apply": { "model": "growthcraft_hops:hops_bush_stage2" }
    },
    {
      "when": { "age": "3" },
      "apply": { "model": "growthcraft_hops:hops_bush_stage3" }
    },
    {
      "apply": { "model": "growthcraft:rope_fence" }
    },
    {
      "when": { "north": "true" },
      "apply": { "model": "growthcraft:rope_fence_side", "uvlock": false }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "growthcraft:rope_fence_side", "y": 90, "uvlock": false }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "growthcraft:rope_fence_side", "y": 180, "uvlock": false }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "growthcraft:rope_fence_side", "y": 270, "uvlock": false }
    },
    {
      "when": { "up": "true" },
      "apply": { "model": "growthcraft:rope_fence_side", "x": 270, "uvlock": false }
    },
    {
      "when": { "down": "true" },
      "apply": { "model": "growthcraft:rope_fence_side", "x": 90, "uvlock": false }
    }
  ]
}